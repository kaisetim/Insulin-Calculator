{
  "extends": ["../../.eslintrc"],
  "parserOptions": {
    "ecmaFeatures": {
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "codeFrame": true,
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": 1
  },
  "globals": { "react": true },
  "env": {
    "browser": true
  }
}
